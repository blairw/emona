<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chartsdemo.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="366.0" layoutY="411.0" onAction="#handleButtonAction" text="Click Me!" />
      <PieChart fx:id="myPieChart" layoutX="14.0" layoutY="14.0" prefHeight="370.0" prefWidth="385.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
      <LineChart fx:id="myLineChart" layoutX="417.0" layoutY="14.0" prefHeight="370.0" prefWidth="385.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <xAxis>
          <CategoryAxis label="Month" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
    </children>
</AnchorPane>
